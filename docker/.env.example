# PostgreSQL Database Configuration
POSTGRES_DB=newtab
POSTGRES_USER=newtab
POSTGRES_PASSWORD=newtab

# JWT Secret for Authentication Service
# Change this to a secure random string (minimum 256 bits)
JWT_SECRET=your-super-secret-key-change-this-in-production-min-256-bits

# PostgreSQL Connection URLs
POSTGRES_URL_AUTH=jdbc:postgresql://postgres:5432/newtab?reWriteBatchedInserts=true
POSTGRES_URL_NEWTAB=jdbc:postgresql://localhost:5432/newtab

# Service Ports
POSTGRES_PORT=5432
AUTH_BE_PORT=8081
NGINX_PORT=80

